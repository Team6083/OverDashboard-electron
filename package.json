    "electron-winstaller": "^2.6.4",
    "mocha": "^5.2.0",
    "spectron": "^3.8.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "canvas-gauges": "^2.1.5",
    "d3": "^5.6.0",
